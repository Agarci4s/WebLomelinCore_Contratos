<script>
    function addReadOnly() {
        var tipo = $("#cmbPersona").val();
        if (tipo == "M") {
            $("#txtNombres").val("");
            $("#txtPaterno").val("");
            $("#txtMaterno").val("");
            $("#txtNombres").attr("readonly", "readonly");
            $("#txtPaterno").attr("readonly", "readonly");
            $("#txtMaterno").attr("readonly", "readonly");
            $("#cmbTipoIdentificacionPF").attr("readonly", "readonly");
            $("#txtNumIdentPF").attr("readonly", "readonly");
            $("#txtRazonSocial").removeAttr("readonly");
        }
        else {
            $("#txtNombres").removeAttr("readonly");
            $("#txtPaterno").removeAttr("readonly");
            $("#txtMaterno").removeAttr("readonly");
            $("#cmbTipoIdentificacionPF").removeAttr("readonly");
            $("#txtNumIdentPF").removeAttr("readonly");
            $("#txtRazonSocial").attr("readonly", "readonly");
            $("#txtRazonSocial").val("");
        }
       
        var url = "/Propietarios/GetRegimenFiscal";

        $.getJSON(url, { id: tipo }, function (data) {
            var item = "";
            $("#cmbRegimenFiscal").empty();
            $.each(data, function (i, RegimenFiscal) {
                item += '<option value="' + RegimenFiscal.value + '">' + RegimenFiscal.text + '</option>'
            });
            $("#cmbRegimenFiscal").html(item);
        });
   
    }

    function soloNumeros(e) {
        var key = window.Event ? e.which : e.keyCode
        return (key >= 48 && key <= 57)
    } 


</script>
