@model WebColliersCore.Models.CgPropietarios


<header>
    <script src="~/js/core.js"></script>
    <script src="//code.jquery.com/jquery-1.12.0.min.js"></script>

    @{
        try
        {
            if (ViewBag.ErrorFisicas)
            {
                <script type="text/javascript">
                    $(document).ready(function () {
                        $("#modalError").modal('show');
                    });
                </script>

            }
        }
        catch (Exception) { }
        try
        {
            if (ViewBag.ErrorMorales)
            {
                <script type="text/javascript">
                    $(document).ready(function () {
                        $("#modalError2").modal('show');
                    });
                </script>
            }
        }
        catch (Exception) { }
        try
        {
            if (ViewBag.ErrorRFC)
            {
                <script type="text/javascript">
                    $(document).ready(function () {
                        $("#modalErrorRFCPropietario").modal('show');
                    });
                </script>
            }
        }
        catch (Exception) { }
        try
        {
            if (ViewBag.ErrorRFCRep)
            {
                <script type="text/javascript">
                    $(document).ready(function () {
                        $("#modalErrorRFCRep").modal('show');
                    });
                </script>
            }
        }
        catch (Exception) { }

        try
        {
            if (ViewBag.ErrorRegimenFiscal)
            {
                <script type="text/javascript">
                    $(document).ready(function () {
                        $("#modalErrorRegimenFiscal").modal('show');
                    });
                </script>
            }
        }
        catch (Exception) { }
    }
</header>

<div class="container">

    <div class="modal fade" id="modalError" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Aviso</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>
                <div class="modal-body">
                    <label>¡Los campos "Nombre", "Apellido Paterno" y "Apellido Materno" son obligatorios!</label>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="modalError2" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Aviso</h4>
