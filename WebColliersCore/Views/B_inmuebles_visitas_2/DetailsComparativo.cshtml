@model WebColliersCore.Models.B_inmuebles_comparativo

<h4>Detalle comparativo</h4>
<hr />
<div class="row">
    <div class="col-md-4">

        <div class="form-group">
            <label asp-for="nombre" class="control-label"></label>
            <input readonly asp-for="nombre" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="direccion" class="control-label"></label>
            <input readonly asp-for="direccion" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="cp" class="control-label"></label>
            <input readonly asp-for="cp" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="estado" class="control-label"></label>
            <input readonly asp-for="estado" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="municipio" class="control-label"></label>
            <input readonly asp-for="municipio" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="colonia" class="control-label"></label>
            <input readonly asp-for="colonia" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="noext" class="control-label"></label>
            <input readonly asp-for="noext" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="manzana" class="control-label"></label>
            <input readonly asp-for="manzana" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="lote" class="control-label"></label>
            <input readonly asp-for="lote" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="referencia_calle" class="control-label"></label>
            <input readonly asp-for="referencia_calle" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="referencia_calle2" class="control-label"></label>
            <input readonly asp-for="referencia_calle2" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="latidud" class="control-label"></label>
            <input readonly asp-for="latidud" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="longitud" class="control-label"></label>
            <input readonly asp-for="longitud" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="link_maps" class="control-label"></label>
            <textarea readonly asp-for="link_maps" class="form-control"></textarea>
        </div>
        <div class="form-group form-check">
            <label class="form-check-label">
                <input disabled class="form-check-input" asp-for="estacionamiento" />
            </label>
        </div>
        <div class="form-group">
            <label asp-for="m2_construccion" class="control-label"></label>
            <input readonly asp-for="m2_construccion" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="m2_precio" class="control-label"></label>
            <input readonly asp-for="m2_precio" class="form-control" />
        </div>
        <div class="form-group">
            @if (ViewBag.regimen == 3 || ViewBag.regimen == 1)
            {
                <label asp-for="renta" class="control-label">Valor comercial</label>
            }
            else
            {
                <label asp-for="renta" class="control-label"></label>
            }
            <input readonly asp-for="renta" class="form-control" />
        </div>

        <div class="form-group">
            <img src="/Images/precarga/@Model.id_b_inmuebles.ToString("000")/@Model.foto" class="img-fluid" style="height:100px" />
        </div>

        <div class="form-group">
            <label asp-for="id_b_cg_region" class="control-label"></label>
            <select disabled asp-for="id_b_cg_region" asp-items=ViewBag.lstRegion class="form-control"></select>
        </div>
        <div class="form-group">
            <label asp-for="id_b_cg_categorias" class="control-label"></label>
            <select disabled asp-items=ViewBag.lstCategorias asp-for="id_b_cg_categorias" class="form-control"></select>
        </div>
    </div>
</div>

